{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false,
      "filter": "apps/**"
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/api/dist/**",
        "apps/web/dist/**",
        "packages/shared/dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    }
  }
}
